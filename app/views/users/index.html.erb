<p id="notice"><%= notice %></p>

<h1>Users</h1>

<table class="table table-hover">
  <thead>
  <tr>
    <th>Username</th>
  </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= link_to user.username, user %></td>
        <%if user.blocked%>
           <td><p class="bg-info"> account frozen</></td>
        <%else%>
          <td></td>
        <%end%>
      </tr>
    <% end %>
  </tbody>
</table>

